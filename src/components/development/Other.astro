---
import OtherApps from "../data/Other.json";
import App from "../App.astro";
---

<h2 class="type-sidelines">
    <span>Other Technologies</span>
</h2>
<div
    class="grid-x grid-margin-x small-up-1 medium-up-2 large-up-3"
    id="development-other"
>
    {
        OtherApps.projects.map((app) => (
            <App
                name={app.name}
                desc={app.desc}
                image={app.image}
                image_alt={app.image_alt}
                url={app.url}
                button_text={app.button_text}
            />
        ))
    }
</div>
